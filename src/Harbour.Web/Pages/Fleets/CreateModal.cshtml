@page
@using Harbour.Localization
@using Harbour.Web.Pages.Fleets
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<HarbourResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Fleet" asp-page="/Fleets/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewFleet"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
